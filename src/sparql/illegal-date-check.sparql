PREFIX dct: <http://purl.org/dc/terms/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix owl: <http://www.w3.org/2002/07/owl#>

# selecting only unique instances of the three variables
SELECT DISTINCT ?term ?property ?value WHERE
{
  ?term rdfs:isDefinedBy <http://purl.obolibrary.org/obo/uberon.owl> .
  VALUES ?property {dct:date dct:issued dct:created}
  # order of the variables in the triple
    ?term ?property ?value .
  FILTER (datatype(?value) != xsd:date || !regex(str(?value), '^\\d{4}-\\d\\d-\\d\\d$'))
}